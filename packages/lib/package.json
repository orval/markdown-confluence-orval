{
    "name": "@markdown-confluence/lib",
    "version": "5.5.2",
    "description": "This library allows you to publish your notes to Confluence",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "sideEffects": false,
    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc --version && tsc",
        "fmt": "npx prettier --write src/",
        "lint": "eslint --ignore-path ../../.eslintignore --ext .js,.ts src/",
        "prettier-check": "npx prettier --check src/",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest"
    },
    "keywords": [
        "markdown",
        "markdown-confluence",
        "confluence",
        "adf",
        "atlassian",
        "atlassian-document-format",
        "publish-confluence",
        "convert-markdown",
        "obsidian-confluence",
        "obsidian-plugin"
    ],
    "author": "andymac4182",
    "license": "Apache 2.0",
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@types/lodash-es": "^4.17.9",
        "@types/markdown-it": "^14.1.2",
        "@types/mime-types": "^2.1.4",
        "@types/node": "^22.5.3",
        "@types/prosemirror-model": "1.17.0",
        "@types/prosemirror-state": "^1.3.0",
        "@types/prosemirror-transform": "^1.4.2",
        "@types/prosemirror-view": "^1.23.1",
        "@types/spark-md5": "^3.0.2",
        "@types/uuid": "^10.0.0",
        "builtin-modules": "4.0.0",
        "esbuild": "0.23.1",
        "husky": "^9.1.5",
        "jest": "^29.6.1",
        "lint-staged": "15.2.10",
        "markdown-it": "^14.1.0",
        "prettier": "3.3.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tslib": "2.7.0",
        "typescript": "5.5.4"
    },
    "dependencies": {
        "@atlaskit/adf-schema": "^40.9.1",
        "@atlaskit/adf-utils": "^19.0.0",
        "@atlaskit/editor-common": "^88.8.1",
        "@atlaskit/editor-json-transformer": "^8.10.9",
        "confluence.js": "^1.6.3",
        "formdata-node": "^6.0.3",
        "glob": "^11.0.0",
        "gray-matter": "^4.0.3",
        "image-size": "^1.0.2",
        "lodash-es": "^4.17.21",
        "markdown-it-table": "^4.1.1",
        "markdown-table": "^3.0.3",
        "prosemirror-markdown": "^1.11.0",
        "prosemirror-model": "1.22.3",
        "sort-any": "^4.0.5",
        "spark-md5": "^3.0.2",
        "uuid": "^10.0.0",
        "yargs": "17.7.2"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/",
        "provenance": true
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/markdown-confluence/markdown-confluence",
        "directory": "packages/lib"
    },
    "homepage": "https://github.com/markdown-confluence/markdown-confluence/tree/main/packages/lib",
    "bugs": {
        "url": "https://github.com/markdown-confluence/markdown-confluence/issues"
    }
}
